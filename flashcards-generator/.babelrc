{
  "env": {
    "client": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": ["last 2 versions", "safari >= 7"]
          },
          "useBuiltIns": "usage"
        }],
        "@babel/react"
      ]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["./src"]
    }],
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-syntax-import-meta",
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    "@babel/plugin-proposal-json-strings"
  ]
}